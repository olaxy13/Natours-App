romOptions(options);\n    return function frameguard(_req, res, next) {\n        res.setHeader('X-Frame-Options', headerValue);\n        next();\n    };\n};\n"},"lineCount":null}},"error":null,"hash":"bf647ba87aca0235c6a6e1b5207ea154","cacheData":{"env":{}}}